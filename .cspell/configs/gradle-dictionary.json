{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "id": "gradle-dictionary",
    "version": "0.2",
    "name": "Gradle Custom Dictionary Configuration",
    "description": "Custom dictionary definition configuration for Gradle.",
    "readonly": true,
    "dictionaryDefinitions": [
        {
            "name": "gradle-dictionary",
            "description": "Gradle Dictionary",
            "type": "S",
            "useCompounds": false,
            "noSuggest": true,
            "path": "../dictionaries/gradle.txt"
        }
    ],
    "dictionaries": ["gradle-dictionary"]
}
